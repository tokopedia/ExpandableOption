<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Top ads widget radio expand view-->
    <declare-styleable name="BaseExpandableOption" >
        <attr name="eo_title" format="reference|string" />
        <attr name="eo_checked" format="boolean"/>
        <attr name="eo_image_arrow_up" format="reference"/>
        <attr name="eo_image_arrow_down" format="reference"/>
        <attr name="eo_use_rotate_animation" format="boolean"/>
        <attr name="eo_theme" format="reference"/>
        <attr name="eo_header_layout" format="reference"/>
        <attr name="eo_enabled" format="reference|boolean"/>
        <attr name="eo_enable_animation" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="ExpandableOptionDefaultTheme">
        <attr name="eo_radio_button_style" format="reference"/>
        <attr name="eo_expandable_style" format="reference"/>
        <attr name="eo_switch_style" format="reference"/>
        <attr name="eo_text_option_style" format="reference"/>
        <attr name="eo_checked_style" format="reference"/>
        <attr name="eo_arrow_style" format="reference"/>
        <attr name="eo_header_style" format="reference"/>
    </declare-styleable>
</resources>